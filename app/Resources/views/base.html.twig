<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('bower_assets/bootstrap/dist/css/bootstrap.css') }}">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body class="container-fluid">
    <div class="row">
        {% include("navbar.html.twig") %}
    </div>

    <div class="row">
        <div class="col-md-7 col-md-offset-1">
        {% block body %}{% endblock %}
    </div>
    </div>
        {% block javascripts %}
            <script src="{{ asset('bower_assets/jquery/dist/jquery.js') }}"></script>
            <script src="{{ asset('bower_assets/bootstrap/dist/js/bootstrap.js') }}"></script>
        {% endblock %}
    </body>
</html>
